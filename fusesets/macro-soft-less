imfuse --diffstat=r2,close2x2   --soft=25 --less=5
imfuse --log=s0.2,close2x2      --soft=25 --less=4
imfuse --dog=s0.3,close2x2      --soft=25 --less=4
imfuse --freichen=15,close2x2   --soft=1  --less=0.1
imfuse --gaussian=s0.3,close2x2 --soft=25 --less=2
imfuse --wavelet=p10,close2x2   --soft=75 --less=35
